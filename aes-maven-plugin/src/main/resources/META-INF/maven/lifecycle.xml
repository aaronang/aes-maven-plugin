<lifecycles>
	<lifecycle>
		<id>aes</id>
		<phases>
			<phase>
				<id>initialize</id>
				<executions>
					<execution>
						<goals>
							<goal>init</goal>
						</goals>
					</execution>
				</executions>
			</phase>
			<phase>
				<id>test-compile</id>
				<configuration>
					<skip>true</skip>
				</configuration>
			</phase>
			<phase>
				<id>test</id>
				<configuration>
					<testFailureIgnore>true</testFailureIgnore>
					<properties>
						<property>
							<name>listener</name>
							<value>pt.up.fe.aes.base.runtime.TestListener</value>
						</property>
					</properties>
				</configuration>
			</phase>
		</phases>
	</lifecycle>
</lifecycles>
